{
  "name": "simplified-registry-admin",
  "version": "1.1.5",
  "description": "Web Front-end for Library Simplified Circulation Manager Admin Interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/NYPL-Simplified/registry_admin.git"
  },
  "main": "dist/registry-admin.js",
  "author": "NYPL",
  "license": "Apache-2.0",
  "scripts": {
    "prepublishOnly": "npm run prod",
    "lint": "tslint -c tslint.json src/*.ts src/*.tsx src/**/*.ts src/**/*.tsx src/**/**/*.ts src/**/**/*.tsx src/**/**/**/*.ts src/**/**/**/*.tsx && sass-lint -c .sass-lint.yml -v -q",
    "test": "npm run lint && tsc && cp -r src/stylesheets lib && mocha --require lib/testHelper.js lib/__tests__/*.js lib/**/__tests__/*.js lib/**/**/__tests__/*.js",
    "test-file": "npm run lint && tsc && cp -r src/stylesheets lib && mocha --require lib/testHelper.js",
    "dev": "webpack --watch --progress --display-modules --config webpack.dev.config",
    "prod": "webpack -p --progress --display-modules --config webpack.prod.config"
  },
  "dependencies": {
    "@nypl/dgx-svg-icons": "^0.3.9",
    "bootstrap": "^3.3.6",
    "library-simplified-reusable-components": "1.3.7",
    "opds-web-client": "0.2.0",
    "prop-types": "^15.7.2",
    "react": "^15.4.0",
    "react-bootstrap": "0.29.4",
    "react-dom": "^15.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "@types/react": "16.4.7",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^2.9.1",
    "fetch-mock": "^7.3.1",
    "file-loader": "^3.0.1",
    "jsdom": "^9.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "node-sass": "4.11.0",
    "react-addons-test-utils": "^15.3.0",
    "sass-lint": "^1.9.1",
    "sass-loader": "^7.1.0",
    "sinon": "7.3.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^4.3.1",
    "tslint": "^3.13.2",
    "typescript": "2.7.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^0.5.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  }
