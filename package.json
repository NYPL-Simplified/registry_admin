{
  "name": "simplified-registry-admin",
  "version": "1.4.8",
  "description": "Web Front-end for Library Simplified Circulation Manager Admin Interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/NYPL-Simplified/registry_admin.git"
  },
  "main": "dist/registry-admin.js",
  "author": "NYPL",
  "license": "Apache-2.0",
  "scripts": {
    "prepublishOnly": "npm run prod",
    "lint": "tslint -c tslint.json src/*.ts src/*.tsx src/**/*.ts src/**/*.tsx src/**/**/*.ts src/**/**/*.tsx src/**/**/**/*.ts src/**/**/**/*.tsx && sass-lint -c .sass-lint.yml -v -q",
    "test": "npm run lint && tsc && cp -r src/stylesheets lib && mocha --require lib/testHelper.js lib/__tests__/*.js lib/**/__tests__/*.js lib/**/**/__tests__/*.js",
    "test-file": "npm run lint && tsc && cp -r src/stylesheets lib && mocha --require lib/testHelper.js",
    "dev": "webpack --watch --progress --display-modules --config webpack.dev.config",
    "dev-test-axe": "TEST_AXE=true npm run dev",
    "prod": "webpack -p --progress --display-modules --config webpack.prod.config",
    "build-docs": "node_modules/.bin/typedoc --tsconfig tsconfig.json"
  },
  "dependencies": {
    "@nypl/dgx-svg-icons": "^0.3.9",
    "bootstrap": "^3.3.6",
    "library-simplified-reusable-components": "^1.3.17",
    "opds-web-client": "0.3.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-bootstrap": "0.32.4",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "recharts": "^1.7.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "terser-webpack-plugin": "^1.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.12.6",
    "@types/prop-types": "^15.7.3",
    "@types/react": "16.9.11",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "fetch-mock": "^7.3.1",
    "file-loader": "^3.0.1",
    "jsdom": "^15.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "node-sass": "4.11.0",
    "react-axe": "^3.3.0",
    "sass-lint": "^1.9.1",
    "sass-loader": "^7.1.0",
    "sinon": "7.3.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-react-a11y": "1.1.0",
    "typedoc": "0.15.3",
    "typescript": "3.7.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "2.2.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  }
}
